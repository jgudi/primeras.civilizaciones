/**
 * Scrambled List iDevice
 *
 * Released under Attribution-ShareAlike 4.0 International License.
 * Author: Ignacio Gros (http://gros.es/) for http://exelearning.net/
 *
 * License: http://creativecommons.org/licenses/by-sa/4.0/
 *
 * It includes the HTML5 Sortable jQuery Plugin, released under the MIT license (details below)
 */
var $exeScrambledList = {
	idevicePath:'',

	init: function () {
		this.idevicePath = (typeof ($exeAuthoring) != 'undefined') ? "/scripts/idevices/scrambled-list/export/" : "";
		$(".exe-sortableList").each(function (instance) {
			if ($('body').hasClass("exe-epub3")) {
				$(this).prepend('<p>' + $exe_i18n.epubDisabled + '</p><p><strong>' + $exe_i18n.solution + ':</strong></p>')
			} else {
				$exeScrambledList.enableList(this, instance);

			}
		});
	},

	enableList: function (activity, instance) {

		var lists = $("ul.exe-sortableList-list", activity);
		var btn = $("p.exe-sortableList-buttonText", activity);
		var evaluationIDC = $('div.exe-sortableList-evaluationID', activity);
		var evaluationID = '';
		var ideviceIDC = $('div.exe-sortableList-ideviceID', activity);
		var ideviceID = false;
		var evaluationMessagesC = $('div.exe-sortableList-evaluationMessages', activity);
		var evaluationMessages = '###'


		if (ideviceIDC.length == 1 && ideviceIDC.text() != "") {
			ideviceID = ideviceIDC.text();
		}
		if (evaluationIDC.length == 1 && evaluationIDC.text() != "") {
			evaluationID = evaluationIDC.text();
		}
		if (evaluationMessagesC.length == 1 && evaluationMessagesC.text() != "") {
			evaluationMessages = evaluationMessagesC.text();
		}
		if (lists.length == 1 && btn.length == 1) {
			lists.css("visibility", "hidden");
			lists.each(
				function () {
					var lis = [];
					$("LI", this).each(function () {
						lis.push(this.innerHTML);
					});
					lis = $exeScrambledList.randomizeArray(lis);
					$exeScrambledList.getListHTML(activity, lis, this, instance, btn, evaluationID, ideviceID, evaluationMessages);
				}
			);

			$exeScrambledList.updateEvaluationIcon(instance, evaluationID, ideviceID, evaluationMessages)
		}

	},


	getLinksHTML: function (i, listOrder) {
		return '<span> <a href="#" class="up exe-sortableList-sorter exe-sortableList-sort-' + i + '_' + (i - 1) + '_' + listOrder + '" title="' + (i + 1) + ' &rarr; ' + i + '"><span class="sr-av">' + (i + 1) + ' &rarr; ' + i + '</span></a> <a href="#" class="down exe-sortableList-sorter exe-sortableList-sort-' + i + '_' + (i + 1) + '_' + listOrder + '" title="' + (i + 1) + ' &rarr; ' + (i + 2) + '"><span class="sr-av">' + (i + 1) + ' &rarr; ' + (i + 2) + '</span></a></span>';
	},

	getListLinks: function (listOrder) {
		var ul = $("#exe-sortableList-" + listOrder);
		var lis = $("li", ul);
		$("SPAN", ul).remove();
		lis.each(function (i) {
			this.className = "";
			if (i == 0) this.className = "first";
			if ((i + 1) == lis.length) this.className = "last";
			this.innerHTML += $exeScrambledList.getLinksHTML(i, listOrder);
		});
		// No inline JavaScript (onclick, etc.)
		$('a.exe-sortableList-sorter', ul).each(function () {
			var c = this.className;
			c = c.split("exe-sortableList-sort-");
			if (c.length == 2) {
				c = c[1].split('_');
				if (c.length == 3) {
					this.onclick = function () {
						$exeScrambledList.sortList(this, parseInt(c[0]), parseInt(c[1]), parseInt(c[2]));
						return false;
					}
				}
			}
		});
	},

	sortList: function (e, a, b, listOrder) { // LI - FROM - TO
		var list = $("#exe-sortableList-" + listOrder);
		list.sortable("destroy");
		var lis = $("LI", list);
		if (b < 0 || b > (lis.length - 1)) return false;
		var newList = [];
		var li, prev, current, next;
		for (var i = 0; i < lis.length; i++) {
			li = lis[i].innerHTML.split("<span>")[0].split("<SPAN>")[0];
			newList.push(li);
			if (i == (a - 1)) prev = li;
			else if (i == a) current = li;
			else if (i == (a + 1)) next = li;
		}
		newList[b] = current;
		if (b < a) { // Up
			newList[a] = prev;
		} else { // Down
			newList[a] = next;
		}
		list.html($exeScrambledList.getULhtml(newList, listOrder)).sortable();
	},

	getULhtml: function (lis, listOrder) {
		html = '';
		for (var i = 0; i < lis.length; i++) {
			html += '<li>' + lis[i] + '</li>';
		}
		$("#exe-sortableList-" + listOrder).html(html).sortable();
		$exeScrambledList.getListLinks(listOrder);
	},

	getListHTML: function (activity, lis, list, listOrder, btn, evaluationID, ideviceID, messages) {
		var html = '<ul class="exe-sortableList-options" id="exe-sortableList-' + listOrder + '">';
		for (var i = 0; i < lis.length; i++) {
			html += '<li>' + lis[i] + '</li>';
		}
		html += "</ul>";
		html += '<p id="exe-sortableListButton-' + listOrder + '"><input type="button" class="feedbackbutton exe-sortableList-check-' + listOrder + '" value="' + btn.text() + '" /></p>';
		html += '<div id="exe-sortableList-' + listOrder + '-feedback"></div>';
		$(list).hide().attr("id", "exe-sortableListResults-" + listOrder).before(html);
		$("#exe-sortableList-" + listOrder).sortable().bind('sortupdate', function (e, ui) {
			$exeScrambledList.getListLinks(listOrder);
		});
		$exeScrambledList.getListLinks(listOrder);
		// Event handlers
		$("#exe-sortableListButton-" + listOrder).click(function () {
			$exeScrambledList.check(this, listOrder, evaluationID, ideviceID, messages);
		});
		// / Event handlers		
	},

	check: function (e, listOrder, evaluationID, ideviceID, messages) {
		// No more sorting 
		$("#exe-sortableListButton-" + listOrder).hide();
		var list = $("#exe-sortableList-" + listOrder);
		$("a", list).hide();
		list.sortable("destroy");
		// Check the answers
		var activity = $(e).parents(".exe-sortableList");
		var right = true;
		var userList = $("#exe-sortableList-" + listOrder);
		var rightAnswers = $("#exe-sortableListResults-" + listOrder);
		var rightAnswersLis = $("li", rightAnswers);
		$("LI", userList).each(function (i) {
			var currentText = $(this).html().split("<span>")[0].split("<SPAN>")[0];
			if (currentText != rightAnswersLis.eq(i).html()) right = false;
		});
		var feedback = $('#exe-sortableList-' + listOrder + '-feedback');
		var score = '0.00'
		if (right) {
			feedback.html("<p>" + $(".exe-sortableList-rightText", activity).text() + "</p>").hide().attr("class", "feedback feedback-right").fadeIn();
			score = '10.00'
		} else {
			feedback.html("<p>" + $(".exe-sortableList-wrongText", activity).text() + "</p><ul>" + rightAnswers.html() + "</ul>").hide().attr("class", "feedback feedback-wrong").fadeIn();
		}
		$exeScrambledList.saveEvaluation(listOrder, evaluationID, ideviceID, score, messages)
	},

	randomizeArray: function (o) {
		var original = [];
		for (var w = 0; w < o.length; w++) original.push(o[w]);
		for (var j, x, i = o.length; i; j = Math.floor(Math.random() * i), x = o[--i], o[i] = o[j], o[j] = x);
		var hasChanged = false;
		for (var y = 0; y < o.length; y++) {
			if (!hasChanged && original[y] != o[y]) hasChanged = true;
		}
		if (hasChanged) return o;
		else return this.randomizeArray(original);
	},
	saveEvaluation: function (instance, evaluationID, ideviceID, score, messages) {
		if (ideviceID && evaluationID && evaluationID.length > 4) {
			var name = $('#exe-sortableList-' + instance).parents('article').find('.iDeviceTitle').eq(0).text(),
				node = $('#nodeTitle').text(),
				formattedDate = $exeScrambledList.getDateString(),
				messagesE = messages.split('#'),
				scorm = {
					'id': ideviceID,
					'type': messagesE[3],
					'node': node,
					'name': name,
					'score': score,
					'date': formattedDate,
					'state': (parseFloat(score) >= 5 ? 2 : 1)
				}
			var data = $exeScrambledList.getDataStorage(evaluationID);
			data = $exeScrambledList.updateEvaluation(data, scorm);
			data = JSON.stringify(data, evaluationID);
			localStorage.setItem('dataEvaluation-' + evaluationID, data);
			$exeScrambledList.showEvaluationIcon(instance, scorm.state, scorm.score, messages)
		}
	},
	updateEvaluationIcon: function (instance, evaluationID, ideviceID, messages) {
		if (ideviceID && evaluationID && evaluationID.length > 4) {
			var node = $('#nodeTitle').text(),
				data = $exeScrambledList.getDataStorage(evaluationID),
				score = '',
				state = 0;
			if (!data) {
				$exeScrambledList.showEvaluationIcon(instance, state, score, messages);
				return;
			}
			const findObject = data.activities.find(
				obj => obj.id == ideviceID && obj.node === node
			);
			if (findObject) {
				state = findObject.state;
				score = findObject.score;
			}
			$exeScrambledList.showEvaluationIcon(instance, state, score, messages);
			var ancla = 'ac-' + ideviceID;
			$('#' + ancla).remove();
			$('#exe-sortableList-' + instance).parents('article').prepend('<div id="' + ancla + '"></div>');

		}
	},
	showEvaluationIcon: function (instance, state, score, messages) {
		var $header = $('#exe-sortableList-' + instance).parents('article').find('header.iDevice_header');
		messages = messages.split('#')
		var icon = 'exequextsq.png',
			alt = messages[0]
		if (state == 1) {
			icon = 'exequextrerrors.png';
			alt = (messages[2]).replace('%s', score);

		} else if (state == 2) {
			icon = 'exequexthits.png';
			alt = (messages[1]).replace('%s', score);
		}
		$('#exe-sortableList-EvaluationIcon-' + instance).remove();
		var sicon = '<div id="exe-sortableList-EvaluationIcon-' + instance + '" class="exe-sortableList-EvaluationDivIcon"><img  src="' + $exeScrambledList.idevicePath + icon + '"><span>' + messages[0] + '</span></div>'
		$header.eq(0).append(sicon);
		$('#exe-sortableList-EvaluationIcon-' + instance).find('span').eq(0).text(alt)
	},
	updateEvaluation: function (obj1, obj2, id1) {
		if (!obj1) {
			obj1 = {
				id: id1,
				activities: []
			};
		}
		const findObject = obj1.activities.find(
			obj => obj.id === obj2.id && obj.node === obj2.node
		);

		if (findObject) {
			findObject.state = obj2.state;
			findObject.score = obj2.score;
			findObject.name = obj2.name;
			findObject.date = obj2.date;
		} else {
			obj1.activities.push({
				'id': obj2.id,
				'type': obj2.type,
				'node': obj2.node,
				'name': obj2.name,
				'score': obj2.score,
				'date': obj2.date,
				'state': obj2.state,
			});
		}
		return obj1;
	},
	getDateString: function () {
		var currentDate = new Date();
		var formattedDate = currentDate.getDate().toString().padStart(2, '0') + '/' +
			(currentDate.getMonth() + 1).toString().padStart(2, '0') + '/' +
			currentDate.getFullYear().toString().padStart(4, '0') + ' ' +
			currentDate.getHours().toString().padStart(2, '0') + ':' +
			currentDate.getMinutes().toString().padStart(2, '0') + ':' +
			currentDate.getSeconds().toString().padStart(2, '0');
		return formattedDate;

	},

	getDataStorage: function (id) {
		var id = 'dataEvaluation-' + id,
			data = $exeScrambledList.isJsonString(localStorage.getItem(id));
		return data;
	},

	isJsonString: function (str) {
		try {
			var o = JSON.parse(str, null, 2);
			if (o && typeof o === "object") {
				return o;
			}
		} catch (e) {}
		return false;
	},

}

$(function () {
	$exeScrambledList.init();
});

/*
 * HTML5 Sortable jQuery Plugin
 * https://github.com/voidberg/html5sortable
